package mx.profuturo.nci.web.beans.traspasos;

import java.io.Serializable;
import java.math.BigDecimal;

public class MonitoreoCargosDomiDetalleBean  implements Serializable 
{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private String recurrencia; 
	private Integer noSolicitudes;
	private Integer traspasoLiq;
	private Integer traspasoPend;
	
	private BigDecimal montoEsperado;
	private Integer cargosAcep;
	private Integer cargosRech;
	private BigDecimal montoConfirmado;
	private BigDecimal montoRech;
	
	private boolean encabezado;
	
	public MonitoreoCargosDomiDetalleBean() {
	}
	
	public MonitoreoCargosDomiDetalleBean(String recurrencia, Integer noSolicitudes, Integer traspasoLiq, 
										  Integer traspasoPend, BigDecimal montoEsperado, Integer cargosAcep, 
										  Integer cargosRech, BigDecimal montoConfirmado, BigDecimal montoRech, boolean encabezado) {
		this.recurrencia = recurrencia;
		this.noSolicitudes = noSolicitudes;
		this.traspasoLiq = traspasoLiq;
		this.traspasoPend = traspasoPend;
		this.montoEsperado = montoEsperado;
		this.cargosAcep = cargosAcep;
		this.cargosRech = cargosRech;
		this.montoConfirmado = montoConfirmado;
		this.montoRech = montoRech;
		this.encabezado = encabezado;
	}
	
	public String getRecurrencia() {
		return recurrencia;
	}
	
	public void setRecurrencia(String recurrencia) {
		this.recurrencia = recurrencia;
	}
	
	public Integer getNoSolicitudes() {
		return noSolicitudes;
	}
	
	public void setNoSolicitudes(Integer noSolicitudes) {
		this.noSolicitudes = noSolicitudes;
	}
	
	public Integer getTraspasoLiq() {
		return traspasoLiq;
	}
	
	public void setTraspasoLiq(Integer traspasoLiq) {
		this.traspasoLiq = traspasoLiq;
	}
	
	public Integer getTraspasoPend() {
		return traspasoPend;
	}
	
	public void setTraspasoPend(Integer traspasoPend) {
		this.traspasoPend = traspasoPend;
	}
	
	public BigDecimal getMontoEsperado() {
		return montoEsperado;
	}
	
	public void setMontoEsperado(BigDecimal montoEsperado) {
		this.montoEsperado = montoEsperado;
	}
	
	public Integer getCargosAcep() {
		return cargosAcep;
	}
	
	public void setCargosAcep(Integer cargosAcep) {
		this.cargosAcep = cargosAcep;
	}
	
	public Integer getCargosRech() {
		return cargosRech;
	}
	
	public void setCargosRech(Integer cargosRech) {
		this.cargosRech = cargosRech;
	}
	
	public BigDecimal getMontoConfirmado() {
		return montoConfirmado;
	}
	
	public void setMontoConfirmado(BigDecimal montoConfirmado) {
		this.montoConfirmado = montoConfirmado;
	}
	
	public BigDecimal getMontoRech() {
		return montoRech;
	}
	
	public void setMontoRech(BigDecimal montoRech) {
		this.montoRech = montoRech;
	}
	
	public boolean isEncabezado() {
		return encabezado;
	}
	
	public void setEncabezado(boolean encabezado) {
		this.encabezado = encabezado;
	}
}