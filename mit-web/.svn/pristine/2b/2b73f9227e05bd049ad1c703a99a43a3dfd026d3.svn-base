package mx.profuturo.nci.web.beans.traspasos;

import java.io.Serializable;
import java.math.BigDecimal;

public class DetalleMesDetalleBean  implements Serializable 
{
	private static final long serialVersionUID = 1L;
	/**
	 * 
	 */
	private String recurrencia;
	private Integer solIngresadas;
	private Integer solAceptadas;
	private Integer solCanceladas;
	private Integer solUnCargoAceptadas;
	private Integer solUnCargoRechazadas;
	private BigDecimal recursosAcreditados;
	private BigDecimal recursosDevoPorTesoreria;
	
	public DetalleMesDetalleBean(){
	}
	
	public DetalleMesDetalleBean(String recurrencia, Integer solIngresadas, Integer solAceptadas, Integer solCanceladas,
			Integer solUnCargoAceptadas, Integer solUnCargoRechazadas, BigDecimal recursosAcreditados, BigDecimal recursosDevoPorTesoreria){
		this.recurrencia = recurrencia;
		this.solIngresadas = solIngresadas;
		this.solAceptadas = solAceptadas;
		this.solCanceladas = solCanceladas;
		this.solUnCargoAceptadas = solUnCargoAceptadas;
		this.solUnCargoRechazadas = solUnCargoRechazadas;
		this.recursosAcreditados = recursosAcreditados;
		this.recursosDevoPorTesoreria = recursosDevoPorTesoreria;
	}
	
	public String getRecurrencia() {
		return recurrencia;
	}
	
	public void setRecurrencia(String recurrencia) {
		this.recurrencia = recurrencia;
	}
	
	public Integer getSolIngresadas() {
		return solIngresadas;
	}
	
	public void setSolIngresadas(Integer solIngresadas) {
		this.solIngresadas = solIngresadas;
	}
	
	public Integer getSolAceptadas() {
		return solAceptadas;
	}
	
	public void setSolAceptadas(Integer solAceptadas) {
		this.solAceptadas = solAceptadas;
	}
	
	public Integer getSolCanceladas() {
		return solCanceladas;
	}
	
	public void setSolCanceladas(Integer solCanceladas) {
		this.solCanceladas = solCanceladas;
	}
	
	public Integer getSolUnCargoAceptadas() {
		return solUnCargoAceptadas;
	}
	
	public void setSolUnCargoAceptadas(Integer solUnCargoAceptadas) {
		this.solUnCargoAceptadas = solUnCargoAceptadas;
	}
	
	public Integer getSolUnCargoRechazadas() {
		return solUnCargoRechazadas;
	}
	
	public void setSolUnCargoRechazadas(Integer solUnCargoRechazadas) {
		this.solUnCargoRechazadas = solUnCargoRechazadas;
	}
	
	public BigDecimal getRecursosAcreditados() {
		return recursosAcreditados;
	}
	
	public void setRecursosAcreditados(BigDecimal recursosAcreditados) {
		this.recursosAcreditados = recursosAcreditados;
	}
	
	public BigDecimal getRecursosDevoPorTesoreria() {
		return recursosDevoPorTesoreria;
	}
	
	public void setRecursosDevoPorTesoreria(BigDecimal recursosDevoPorTesoreria) {
		this.recursosDevoPorTesoreria = recursosDevoPorTesoreria;
	}
}