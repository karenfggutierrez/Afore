package mx.profuturo.nci.web.beans;

import java.util.Date;

public class RangoFechaParamBean {
	
	private Date fechaIni;
	private Date fechaFin;
	
	public Date getFechaIni() {
		return fechaIni;
	}
	public RangoFechaParamBean setFechaIni(Date fechaIni) {
		this.fechaIni = fechaIni;
		return this;
	}
	public Date getFechaFin() {
		return fechaFin;
	}
	public RangoFechaParamBean setFechaFin(Date fechaFin) {
		this.fechaFin = fechaFin;
		return this;
	}

	@Override
	public String toString() {
		String string = null;
		string = ((fechaIni!=null?fechaIni.toString():null)+"-"+(fechaFin!=null?fechaFin.toString():null));
		return string;
	}
}
