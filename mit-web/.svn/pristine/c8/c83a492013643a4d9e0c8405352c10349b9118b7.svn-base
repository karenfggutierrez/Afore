package mx.profuturo.nci.web.beans.traspasos;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.List;

public class InversionDomiTraspasosBean implements Serializable 
{
	private static final long serialVersionUID = 1L;
	/**
	 * 
	 */
	private BigDecimal importeTot;
	private Integer registrosTot;
	private List<InversionDomiTraspasosDetalleBean> lstRecurrencias;
	
	public BigDecimal getImporteTot() {
		return importeTot;
	}
	
	public void setImporteTot(BigDecimal importeTot) {
		this.importeTot = importeTot;
	}
	
	public Integer getRegistrosTot() {
		return registrosTot;
	}
	
	public void setRegistrosTot(Integer registrosTot) {
		this.registrosTot = registrosTot;
	}
	
	public List<InversionDomiTraspasosDetalleBean> getLstRecurrencias() {
		return lstRecurrencias;
	}
	
	public void setLstRecurrencias(List<InversionDomiTraspasosDetalleBean> lstRecurrencias) {
		this.lstRecurrencias = lstRecurrencias;
	}
}