package mx.profuturo.nci.web.beans;

import java.math.BigDecimal;

public class CifrasBancoBean {
	private GenericCatalogoBean banco;
	private GenericCatalogoBean bancoIncluir;
	private Long totalRegistros;
	private BigDecimal importeTotal;

	public GenericCatalogoBean getBanco() {
		return banco;
	}

	public CifrasBancoBean setBanco(GenericCatalogoBean banco) {
		this.banco = banco;
		return this;
	}

	public Long getTotalRegistros() {
		return totalRegistros;
	}

	public CifrasBancoBean setTotalRegistros(Long totalRegistros) {
		this.totalRegistros = totalRegistros;
		return this;
	}

	public BigDecimal getImporteTotal() {
		return importeTotal;
	}

	public CifrasBancoBean setImporteTotal(BigDecimal importeTotal) {
		this.importeTotal = importeTotal;
		return this;
	}

	public GenericCatalogoBean getBancoIncluir() {
		return bancoIncluir;
	}

	public void setBancoIncluir(GenericCatalogoBean bancoIncluir) {
		this.bancoIncluir = bancoIncluir;
	}
	
}
