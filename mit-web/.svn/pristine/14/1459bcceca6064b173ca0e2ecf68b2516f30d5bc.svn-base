package mx.profuturo.nci.web.beans;

import java.io.Serializable;
import java.util.Date;

public class ValidacionesBean implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private GenericCatalogoBean tipoValidacion;
	private GenericCatalogoBean estatus;
	private GenericCatalogoBean motivoRechazo;
	
	public GenericCatalogoBean getTipoValidacion() {
		return tipoValidacion;
	}
	
	public void setTipoValidacion(GenericCatalogoBean tipoValidacion) {
		this.tipoValidacion = tipoValidacion;
	}
	
	public GenericCatalogoBean getEstatus() {
		return estatus;
	}
	
	public void setEstatus(GenericCatalogoBean estatus) {
		this.estatus = estatus;
	}
	
	public GenericCatalogoBean getMotivoRechazo() {
		return motivoRechazo;
	}
	
	public void setMotivoRechazo(GenericCatalogoBean motivoRechazo) {
		this.motivoRechazo = motivoRechazo;
	}	
}