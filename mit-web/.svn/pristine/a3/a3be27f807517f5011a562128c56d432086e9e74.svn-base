ul.paginacion{overflow:hidden;list-style:none;margin:0;padding:0}ul.paginacion li{float:left;margin:0 0;font-size:12px;font-weight:700}ul.paginacion li a{display:block;padding:8px 15px;text-decoration:none!important;color:#00427a;border-left:1px solid #ddd;border-top:1px solid #ddd;border-bottom:1px solid #ddd}ul.paginacion li .actual{background:#f3f3f3;color:#99999e}ul.paginacion li .anterior{border-radius:4px 0 0 4px}ul.paginacion li .siguiente{border-right:1px solid #ddd;border-radius:0 4px 4px 0}