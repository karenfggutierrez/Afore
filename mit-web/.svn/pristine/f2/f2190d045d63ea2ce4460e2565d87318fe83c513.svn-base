	public BigDecimal getTotalDiver(){
		BigDecimal reg = BigDecimal.ZERO;
		if(cpDosMeses != null){
			reg = reg.add(cpDosMeses);
		}
		if(lpDoceMeses != null){
			reg = reg.add(lpDoceMeses);
		}
		if(avpilp != null){
			reg = reg.add(avpilp);
		}
		if(acr != null){
			reg = reg.add(acr);
		}
		return reg;		
	}
	
