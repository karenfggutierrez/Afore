package mx.profuturo.nci.business.vo;

import java.util.Date;

import org.apache.commons.lang3.builder.ToStringBuilder;
import org.apache.commons.lang3.builder.ToStringStyle;

public class CatalogoVO {

  private Short idCatCatalogo;
  private TipoCatalogoVO catalogType;
  private Short idCatPadre;
  private String valor;
  private String desc;
  private Boolean vigencia;
  private Date fehCre;
  private String usuCre;
  private Date fehAct;
  private String usuAct;

  public CatalogoVO() {
  }

  public Short getIdCatCatalogo() {
    return idCatCatalogo;
  }

  public CatalogoVO setIdCatCatalogo(final Short idCatCatalogo) {
    this.idCatCatalogo = idCatCatalogo;

    return this;
  }

  public TipoCatalogoVO getCatalogType() {
    return catalogType;
  }

  public CatalogoVO setCatalogType(final TipoCatalogoVO catalogType) {
    this.catalogType = catalogType;

    return this;
  }

  public Short getIdCatPadre() {
    return idCatPadre;
  }

  public CatalogoVO setIdCatPadre(final Short idCatPadre) {
    this.idCatPadre = idCatPadre;

    return this;
  }

  public String getValor() {
    return valor;
  }

  public CatalogoVO setValor(final String valor) {
    this.valor = valor;

    return this;
  }

  public String getDesc() {
    return desc;
  }

  public CatalogoVO setDesc(final String desc) {
    this.desc = desc;

    return this;
  }

  public Boolean getVigencia() {
    return vigencia;
  }

  public CatalogoVO setVigencia(final Boolean vigencia) {
    this.vigencia = vigencia;

    return this;
  }

  public Date getFehCre() {
    return fehCre;
  }

  public CatalogoVO setFehCre(final Date fehCre) {
    this.fehCre = fehCre;

    return this;
  }

  public String getUsuCre() {
    return usuCre;
  }

  public CatalogoVO setUsuCre(final String usuCre) {
    this.usuCre = usuCre;

    return this;
  }

  public Date getFehAct() {
    return fehAct;
  }

  public CatalogoVO setFehAct(final Date fehAct) {
    this.fehAct = fehAct;

    return this;
  }

  public String getUsuAct() {
    return usuAct;
  }

  public CatalogoVO setUsuAct(final String usuAct) {
    this.usuAct = usuAct;

    return this;
  }

  @Override
  public String toString() {
    return new ToStringBuilder(this, ToStringStyle.DEFAULT_STYLE).appendSuper(super.toString())
        .append("idCatCatalogo", idCatCatalogo).append("catalogType", catalogType).append("idCatPadre", idCatPadre)
        .append("valor", valor).append("desc", desc).append("vigencia", vigencia).append("fehCre", fehCre)
        .append("usuCre", usuCre).append("fehAct", fehAct).append("usuAct", usuAct).toString();
  }

}
