package mx.profuturo.nci.business.vo;

public class CifrasTotalesDomiVO {
	private String idPerido;
	private String descPeriodo;
	private String pesos;
	
	public CifrasTotalesDomiVO () {
		
	}
	
	public CifrasTotalesDomiVO ( String idPerido
							   , String descPeriodo
							   , String pesos) {
		this.idPerido = idPerido;
		this.descPeriodo = descPeriodo;
		this.pesos = pesos;
	}

	public String getIdPerido() {
		return idPerido;
	}

	public void setIdPerido(String idPerido) {
		this.idPerido = idPerido;
	}

	public String getDescPeriodo() {
		return descPeriodo;
	}

	public void setDescPeriodo(String descPeriodo) {
		this.descPeriodo = descPeriodo;
	}

	public String getPesos() {
		return pesos;
	}

	public void setPesos(String pesos) {
		this.pesos = pesos;
	}
	
	
}
