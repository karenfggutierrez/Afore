package mx.profuturo.nci.business.report.beans;

import java.util.HashMap;
import java.util.List;
import java.util.Map;

public class BasicParamReportBean <T> {
	private String jrxmlReportPath;
	private String outputReportFileName;
	private Map<String, Object> mapParameters=new HashMap<String, Object>();
	private List<T> dataSource;
	
	public Map<String, Object> getMapParameters() {
		return mapParameters;
	}
	public void setMapParameters(Map<String, Object> mapParameters) {
		this.mapParameters = mapParameters;
	}
	public String getJrxmlReportPath() {
		return jrxmlReportPath;
	}
	public void setJrxmlReportPath(String jrxmlReportPath) {
		this.jrxmlReportPath = jrxmlReportPath;
	}
	
	public List<T> getDataSource() {
		return dataSource;
	}
	public void setDataSource(List<T> dataSource) {
		this.dataSource = dataSource;
	}
	public String getOutputReportFileName() {
		return outputReportFileName;
	}
	public void setOutputReportFileName(String outputReportFileName) {
		this.outputReportFileName = outputReportFileName;
	}
	
	  
}
