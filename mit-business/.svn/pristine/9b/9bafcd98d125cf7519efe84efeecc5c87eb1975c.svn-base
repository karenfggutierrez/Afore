package mx.profuturo.nci.business.vo;

public class MapaReferenciasVO  extends AbstractAuditoriaVO
{
	private Short idconfiguracion;
	private Short claveNCI;
	private String descripcionNCI;
	private Integer claveItgy;
	private String descripcionItgy;
	private Short vigencia;
	private String uso;
	public Short getIdconfiguracion() {
		return idconfiguracion;
	}
	public void setIdconfiguracion(Short idconfiguracion) {
		this.idconfiguracion = idconfiguracion;
	}
	public Short getClaveNCI() {
		return claveNCI;
	}
	public void setClaveNCI(Short claveNCI) {
		this.claveNCI = claveNCI;
	}
	public String getDescripcionNCI() {
		return descripcionNCI;
	}
	public void setDescripcionNCI(String descripcionNCI) {
		this.descripcionNCI = descripcionNCI;
	}
	public Integer getClaveItgy() {
		return claveItgy;
	}
	public void setClaveItgy(Integer claveItgy) {
		this.claveItgy = claveItgy;
	}
	public String getDescripcionItgy() {
		return descripcionItgy;
	}
	public void setDescripcionItgy(String descripcionItgy) {
		this.descripcionItgy = descripcionItgy;
	}
	public Short getVigencia() {
		return vigencia;
	}
	public void setVigencia(Short vigencia) {
		this.vigencia = vigencia;
	}
	public String getUso() {
		return uso;
	}
	public void setUso(String uso) {
		this.uso = uso;
	}
}
