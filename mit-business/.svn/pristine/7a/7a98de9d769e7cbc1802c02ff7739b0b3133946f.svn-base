package mx.profuturo.nci.business.wrapped;

import java.io.Serializable;

/**
 * Clase que contiene los parametros de entrada (filter)
 * 
 * @author E0000548 | HÃ©ctor Manuel Loza Cruz
 * @category MANTENIMIENTO
 * @date 20/04/2022
 */
public class TipoProcesamientoDomisFilter implements Serializable{

	private static final long serialVersionUID = -6773154829046200332L;
	private String valor;
	private String usuario;
	
	/**
	 * Constuctor
	 */
	public TipoProcesamientoDomisFilter() {
	}
	
	/**
	 * Constuctor
	 * @param valor
	 * @param usuario
	 */
	public TipoProcesamientoDomisFilter(String valor, String usuario) {
		this.valor = valor;
		this.usuario = usuario;
	}
	
	public String getValor() {
		return valor;
	}
	public void setValor(String valor) {
		this.valor = valor;
	}
	public String getUsuario() {
		return usuario;
	}
	public void setUsuario(String usuario) {
		this.usuario = usuario;
	}

}
