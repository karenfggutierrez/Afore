package mx.profuturo.nci.business.vo;

import java.util.List;

public class CifrasGeneralesTipoVO {

	private List<SeccionVO> seccionesCortoPlazo;
	private List<SeccionVO> seccionesLargoPlazo;
	private ElementoVO totalGeneralConciliado;
	private ElementoVO totalGeneralNoConciliado;
	
	public List<SeccionVO> getSeccionesCortoPlazo() {
		return seccionesCortoPlazo;
	}
	public void setSeccionesCortoPlazo(List<SeccionVO> seccionesCortoPlazo) {
		this.seccionesCortoPlazo = seccionesCortoPlazo;
	}
	public List<SeccionVO> getSeccionesLargoPlazo() {
		return seccionesLargoPlazo;
	}
	public void setSeccionesLargoPlazo(List<SeccionVO> seccionesLargoPlazo) {
		this.seccionesLargoPlazo = seccionesLargoPlazo;
	}
	public ElementoVO getTotalGeneralConciliado() {
		return totalGeneralConciliado;
	}
	public void setTotalGeneralConciliado(ElementoVO totalGeneralConciliado) {
		this.totalGeneralConciliado = totalGeneralConciliado;
	}
	public ElementoVO getTotalGeneralNoConciliado() {
		return totalGeneralNoConciliado;
	}
	public void setTotalGeneralNoConciliado(ElementoVO totalGeneralNoConciliado) {
		this.totalGeneralNoConciliado = totalGeneralNoConciliado;
	}
	
	
}
