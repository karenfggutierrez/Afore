package mx.profuturo.nci.business.vo;

public class ParamCataConfigVO extends AbstractAuditoriaVO{

	private Short idCatalogoConfiguracion;
	private Short idTipoCat;
	private String valor;
	private String descripcion;
  	private Boolean vigencia;
  	
  	public Short getIdCatalogoConfiguracion() {
		return idCatalogoConfiguracion;
	}
  	
	public void setIdCatalogoConfiguracion(Short idCatalogoConfiguracion) {
		this.idCatalogoConfiguracion = idCatalogoConfiguracion;
	}
	
	public Short getIdTipoCat() {
		return idTipoCat;
	}
	
	public void setIdTipoCat(Short idTipoCat) {
		this.idTipoCat = idTipoCat;
	}
	
	public String getValor() {
		return valor;
	}
	
	public void setValor(String valor) {
		this.valor = valor;
	}
	
	public String getDescripcion() {
		return descripcion;
	}
	
	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}
	
	public Boolean getVigencia() {
		return vigencia;
	}
	
	public void setVigencia(Boolean vigencia) {
		this.vigencia = vigencia;
	}
}