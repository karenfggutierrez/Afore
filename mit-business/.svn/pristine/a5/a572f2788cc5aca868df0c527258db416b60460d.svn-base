package mx.profuturo.nci.business.vo.cif;

public class DevolucionesDetalleCifVO {

	private String businessUnit;
	private String origin;
	private String pfTranId;  //idItemDev DevolucionesCIFDetVO
	private Short seqNo;
	private String pfTipoAfectacion;
	private String pfSTipoAfectacion;
	private Double amtSel;		//monto DevolucionesCIFDetVO
	private Short percentage;
	
	public DevolucionesDetalleCifVO() {
	}

	public DevolucionesDetalleCifVO(String idItemDev, Double monto) {
		this.pfTranId = idItemDev;
		this.amtSel = monto;
		this.businessUnit = "PAF01";
		this.origin = "DEV";
		this.seqNo = 1;
		this.pfTipoAfectacion = "DEVDOM";
		this.pfSTipoAfectacion = "DEVDOM";
		this.percentage = 0;
	}
	
	
	public String getBusinessUnit() {
		return businessUnit;
	}
	public void setBusinessUnit(String businessUnit) {
		this.businessUnit = businessUnit;
	}
	public String getOrigin() {
		return origin;
	}
	public void setOrigin(String origin) {
		this.origin = origin;
	}
	public String getPfTranId() {
		return pfTranId;
	}
	public void setPfTranId(String pfTranId) {
		this.pfTranId = pfTranId;
	}
	public Short getSeqNo() {
		return seqNo;
	}
	public void setSeqNo(Short seqNo) {
		this.seqNo = seqNo;
	}
	public String getPfTipoAfectacion() {
		return pfTipoAfectacion;
	}
	public void setPfTipoAfectacion(String pfTipoAfectacion) {
		this.pfTipoAfectacion = pfTipoAfectacion;
	}
	public String getPfSTipoAfectacion() {
		return pfSTipoAfectacion;
	}
	public void setPfSTipoAfectacion(String pfSTipoAfectacion) {
		this.pfSTipoAfectacion = pfSTipoAfectacion;
	}
	public Double getAmtSel() {
		return amtSel;
	}
	public void setAmtSel(Double amtSel) {
		this.amtSel = amtSel;
	}
	public Short getPercentage() {
		return percentage;
	}
	public void setPercentage(Short percentage) {
		this.percentage = percentage;
	}	
	
}
