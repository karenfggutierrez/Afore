package mx.profuturo.nci.business.vo;


import java.io.Serializable;
import java.util.Date;

public class ConfPermisosSeccionVO {
    private Short idSeccion;
    private Short idPerfil;
    private String vigencia;
    private Date fehCre;
    private String usuCre;
    private Date fehAct;
    private String usuAct;
    private static final long serialVersionUID = 1L;
    public Short getIdSeccion() {
        return idSeccion;
    }

    public void setIdSeccion(Short idSeccion) {
        this.idSeccion = idSeccion;
    }

    public Short getIdPerfil() {
        return idPerfil;
    }

    public void setIdPerfil(Short idPerfil) {
        this.idPerfil = idPerfil;
    }
    public String getVigencia() {
        return vigencia;
    }

    public void setVigencia(String vigencia) {
        this.vigencia = vigencia;
    }

    public Date getFehCre() {
        return fehCre;
    }

    public void setFehCre(Date fehCre) {
        this.fehCre = fehCre;
    }

    public String getUsuCre() {
        return usuCre;
    }

    public void setUsuCre(String usuCre) {
        this.usuCre = usuCre;
    }

    public Date getFehAct() {
        return fehAct;
    }

    public void setFehAct(Date fehAct) {
        this.fehAct = fehAct;
    }

    public String getUsuAct() {
        return usuAct;
    }

    public void setUsuAct(String usuAct) {
        this.usuAct = usuAct;
    }
}
